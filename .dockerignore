//Ignore node modules and dependencies
node_modules;
npm -
  debug.log * yarn -
  debug.log * yarn -
  error.log * pnpm -
  debug.log *
    // Logs
    logs *
    // Environment variables
    log.env.env.local.env.development.env.production.env.test.env *
    local;

// Docker files
Dockerfile.dockerignore;

// Build artifacts
build;
dist *
  // IDE/editor config files
  // Git files
  tsbuildinfo.git.gitignore.vscode.idea *
  suo *
  ntvs *
  njsproj *
  // MacOS system files
  sln.DS_Store *
  // Miscellaneous
  swp;
